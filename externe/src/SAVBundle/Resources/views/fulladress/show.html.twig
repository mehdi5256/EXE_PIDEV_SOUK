{% extends 'base.html.twig' %}

{% block body %}
    <h1>Fulladress</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fullAdress.id }}</td>
            </tr>
            <tr>
                <th>Rue</th>
                <td>{{ fullAdress.rue }}</td>
            </tr>
            <tr>
                <th>Nummaison</th>
                <td>{{ fullAdress.numMaison }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ fullAdress.ville }}</td>
            </tr>
            <tr>
                <th>Codepostal</th>
                <td>{{ fullAdress.codePostal }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ fullAdress.pays }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('SAV_fullAdress_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('SAV_fullAdress_edit', { 'id': fullAdress.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
